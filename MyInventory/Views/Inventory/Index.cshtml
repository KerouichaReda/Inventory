@{
  ViewBag.Title = "My Inventory";
}
<h2>Inventory</h2>
<section id="alertas"></section>
<div class="col-lg-5 col-md-6 col-sm-7 col-xs-12">
  <table class="table table-condensed table-striped" id="groupTotals">
    <colgroup>
      <col class="col-lg-6 col-md-6 col-sm-6 col-xs-6" />
      <col class="col-lg-2 col-md-2 col-sm-2 col-xs-2" />
      <col class="col-lg-2 col-md-2 col-sm-2 col-xs-2" />
      <col class="col-lg-2 col-md-2 col-sm-2 col-xs-2" />
    </colgroup>
    <thead>
      <tr>
        <th>Item</th>
        <th>Total</th>
        <th>Caps</th>
        <th></th>
      </tr>
    </thead>
    <tfoot></tfoot>
    <tbody></tbody>
  </table>
</div>
<div class="clearfix"></div>
@section scripts{
  @Scripts.Render("~/bundlesview/inventory-index")
}
@model MyInventory.Models.Inventory.DifferenceViewModel
@{
  ViewBag.Title = "Difference";
}
<h2>@ViewBag.Title <img id="imgLoading" src='@Url.Content("~/images/loading.gif")' style="vertical-align:baseline;height: 20px;display:none" /></h2>
<h3 id="title"></h3>
<div class="form-horizontal col-lg-12">
  <div class="form-group">
    <div class="col-lg-12">
      <button type="button" class="btn btn-primary refresh">Refresh</button>
    </div>
  </div>
</div>
<section id="differenceDetail"></section>
<div class="form-horizontal col-lg-12">
  <div class="form-group">
    <div class="col-lg-12">
      <button type="button" class="btn btn-primary refresh">Refresh</button>
    </div>
  </div>
</div>
<div class="clearfix"></div>
<div>
  <p class="bottomLinks">
    @Html.ActionLink("Back to Inventory", "Index")
  </p>
</div>
@section scripts{
  @Scripts.Render("~/bundlesview/inventory-difference")
}
